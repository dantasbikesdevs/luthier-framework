{
  "name": "dantas/luthier",
  "description": "Framework PHP interno para construção de aplicações MVC",
  "version": "0.1.0",
  "type": "library",
  "authors": [
    {
      "name": "equipe web",
      "email": "examples@example.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Luthier\\": "src/"
    }
  },
  "scripts": {
    "dev": "xdg-open 127.0.0.1:8811; php -S 127.0.0.1:8811;",
    "test": "./vendor/bin/pest",
    "coverage": "./vendor/bin/pest --coverage"
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "firebase/php-jwt": "dev-main",
    "monolog/monolog": "^3.0",
    "phpmailer/phpmailer": "^6.6"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
