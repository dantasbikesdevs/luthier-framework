{
  "name": "dantas/luthier",
  "description": "Framework PHP interno para construção de aplicações MVC",
  "version": "1.0.1",
  "type": "library",
  "authors": [
    {
      "name": "equipe web",
      "email": "programacao@dantaseletro.com.br"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Luthier\\": "src/"
    }
  },
  "scripts": {
    "dev": "xdg-open 127.0.0.1:8811; php -S 127.0.0.1:8811;",
    "test": "./vendor/bin/pest",
    "coverage": "./vendor/bin/pest --coverage"
  },
  "require-dev": {
    "pestphp/pest": "^1.21"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "firebase/php-jwt": "^6.3.0"
  }
}
